{
  "extends": ["tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "prefer-for-of": false,
    "variable-name": ["allow-trailing-underscore"],
    "comment-format": [false],
    "jsx-boolean-value": false
  }
}
